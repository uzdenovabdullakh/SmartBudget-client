{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": true,
        "singleQuote": false,
        "tabWidth": 2,
        "trailingComma": "all",
        "printWidth": 80,
        "endOfLine": "auto"
      }
    ],
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "warn"
  }
}